    "elm-review": "^2.7.2",
    "elm-test": "^0.19.1-revision7",
    "elm-webpack-loader": "^8.0.0",
    "html-webpack-plugin": "^5.5.0",
    "prettier": "^2.6.2",
    "prettier-plugin-elm": "^0.8.0",
    "webpack": "^5.72.0",
    "webpack-cli": "^4.9.2",
    "webpack-dev-server": "^4.8.1"
  }
